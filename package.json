{
    "name": "cloud-react",
    "version": "0.0.79",
    "description": "数据赢家 react 组件",
    "main": "cloud-react.js",
    "scripts": {
        "precommit": "npm run lint",
        "lint": "eslint src --fix",
        "prettier": "prettier -c --write 'src/**/*.{js,less}'",
        "pretty-quick": "pretty-quick --pattern 'src/**/*.{js,less}'",
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "gen-index": "node ./scripts/generate-index.js",
        "start": "npm run gen-index && npm run webpack-dev-server -- --env.mode development --env.domain develop",
        "build": "npm run gen-index && npm run webpack -- --env.mode production --env.domain production",
        "site": "npm run gen-index && npm run webpack -- --env.mode site --env.domain production",
        "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --env.mode production",
        "preversion": "npm run lint",
        "version": "npm run build && npm publish dist",
        "postversion": "git push && git push --tags",
        "test": "jest --coverage || true",
        "test:watch": "jest --watch --coverage || true",
        "test:update": "jest --coverage --no-cache --update-snapshot",
        "test:debug": "node --inspect --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
        "report-coverage": "codecov",
        "new:component": "cp -r tpls/new-component src/components/new-component"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ShuyunFF2E/cloud-react.git"
    },
    "keywords": [
        "react",
        "component"
    ],
    "author": "https://github.com/shuyunFF2E",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-proposal-decorators": "^7.10.1",
        "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
        "@babel/plugin-proposal-function-sent": "^7.10.1",
        "@babel/plugin-proposal-numeric-separator": "^7.10.1",
        "@babel/plugin-proposal-throw-expressions": "^7.10.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@babel/runtime": "^7.10.2",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.0.0-beta.4",
        "clean-webpack-plugin": "^1.0.1",
        "codecov": "^3.7.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-import-resolver-webpack": "^0.11.1",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jest": "^23.13.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "7.20.0",
        "file-loader": "^3.0.1",
        "front-matter": "^3.0.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.0.1",
        "jest-transform-css": "^2.0.0",
        "less": "^3.11.3",
        "less-loader": "^4.1.0",
        "lint-staged": "^10.2.10",
        "loader-utils": "^2.0.0",
        "markdown-it": "^8.4.2",
        "mini-css-extract-plugin": "^0.5.0",
        "mockdate": "^3.0.2",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "prettier": "^1.17.1",
        "pretty-quick": "^2.0.1",
        "react-test-renderer": "^16.13.1",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.5",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1"
    },
    "dependencies": {
        "classnames": "2.2.6",
        "gridmanager-react": "1.7.0",
        "prop-types": "15.7.2",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-router-dom": "5.0.0",
        "shuyun-utils": "0.0.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "post-commmit": "git update-index -g"
        }
    },
    "lint-staged": {
        "src/**/*.{js,less}": [
            "pretty-quick —-staged",
            "git add"
        ]
    },
    "bugs": {
        "url": "https://github.com/ShuyunFF2E/cloud-react/issues"
    },
    "homepage": "https://github.com/ShuyunFF2E/cloud--react#readme"
}
